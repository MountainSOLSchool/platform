{
    "name": "auth-user",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "sourceRoot": "src",
    "prefix": "sol",
    "targets": {
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/libs/auth/user"],
            "options": {
                "jestConfig": "libs/auth/user/jest.config.ts",
                "passWithNoTests": true
            }
        },
        "lint": {
            "executor": "@nx/linter:eslint",
            "options": {
                "lintFilePatterns": [
                    "libs/auth/user/src/**/*.ts",
                    "libs/auth/user/src/**/*.html"
                ]
            }
        }
    },
    "tags": ["scope:auth", "type:api"]
}
