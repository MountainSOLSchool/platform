{
    "name": "request",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "angular/request/src",
    "prefix": "lib",
    "tags": [],
    "projectType": "library",
    "targets": {
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "angular/request/jest.config.ts"
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": [
                    "angular/request/**/*.ts",
                    "angular/request/**/*.html"
                ]
            }
        }
    }
}
