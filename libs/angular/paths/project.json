{
    "name": "paths",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "sourceRoot": "src",
    "prefix": "sol",
    "targets": {
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/libs/paths"],
            "options": {
                "jestConfig": "libs/paths/jest.config.ts",
                "passWithNoTests": true
            }
        },
        "lint": {
            "executor": "@nx/linter:eslint",
            "options": {
                "lintFilePatterns": [
                    "libs/paths/src/**/*.ts",
                    "libs/paths/src/**/*.html"
                ]
            },
            "outputs": ["{options.outputFile}"]
        }
    },
    "tags": ["scope:libs/paths"]
}
