{
    "name": "header",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "sourceRoot": "src",
    "prefix": "sol",
    "targets": {
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/libs/header"],
            "options": {
                "jestConfig": "libs/header/jest.config.ts",
                "passWithNoTests": true
            }
        },
        "lint": {
            "executor": "@nx/linter:eslint",
            "options": {
                "lintFilePatterns": [
                    "libs/header/src/**/*.ts",
                    "libs/header/src/**/*.html"
                ]
            }
        }
    },
    "tags": ["scope:header"]
}
