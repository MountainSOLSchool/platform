        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": [
                    "libs/workflow/**/*.ts",
                    "libs/workflow/**/*.html"
                ]
            }
        }
    },
    "tags": []
