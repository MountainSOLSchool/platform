{
    "name": "table-html",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/table/html/src",
    "projectType": "library",
    "targets": {
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": ["libs/table/html/**/*.ts"]
            },
            "outputs": ["{options.outputFile}"]
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/libs/table/html"],
            "options": {
                "jestConfig": "libs/table/html/jest.config.ts",
                "passWithNoTests": true
            }
        }
    },
    "tags": ["scope:table", "type:api"]
}
