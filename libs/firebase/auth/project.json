{
    "name": "firebase-auth",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "sourceRoot": "libs/firebase/auth/src",
    "prefix": "sol",
    "targets": {
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/libs/firebase/auth"],
            "options": {
                "jestConfig": "libs/firebase/auth/jest.config.ts",
                "passWithNoTests": true
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint",
            "options": {
                "lintFilePatterns": [
                    "libs/firebase/auth/src/**/*.ts",
                    "libs/firebase/auth/src/**/*.html"
                ]
            }
        }
    },
    "tags": ["scope:firebase"]
}
