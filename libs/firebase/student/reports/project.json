{
    "name": "student-reports",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "src",
    "projectType": "library",
    "targets": {
        "lint": {
            "executor": "@nx/eslint:lint",
            "options": {
                "lintFilePatterns": ["libs/student/reports/**/*.ts"]
            },
            "outputs": ["{options.outputFile}"]
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/libs/student/reports"],
            "options": {
                "jestConfig": "libs/student/reports/jest.config.ts",
                "passWithNoTests": true
            }
        }
    },
    "tags": ["scope:student"]
}
