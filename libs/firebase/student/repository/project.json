{
    "name": "student-repository",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "sourceRoot": "src",
    "prefix": "sol",
    "targets": {
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": [
                "{workspaceRoot}/coverage/libs/firebase/student/repository"
            ],
            "options": {
                "jestConfig": "libs/firebase/student/repository/jest.config.ts"
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint",
            "options": {
                "lintFilePatterns": [
                    "libs/firebase/student/repository/src/**/*.ts",
                    "libs/firebase/student/repository/src/**/*.html"
                ]
            }
        }
    },
    "tags": ["scope:student"]
}
