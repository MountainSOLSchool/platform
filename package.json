        "build:functions": "nx dep-graph --file=output.html && rm -rf dist && npm run nx build functions",
        "build:functions:watch": "nx dep-graph --file=output.html && rm -rf dist && nx build functions --watch",
        "build:portal": "nx build portal --outputPath=\"public/portal\"",
        "build:portal2": "nx export portal-react --outputPath=\"public/portal2\"",
        "serve:functions": "npm run build:functions:watch & (sleep 5 && firebase serve --only functions --project=dev --port=5001) && fg",
        "serve:dev:portal": "nx serve portal",
        "serve:portal": "nx serve portal --configuration=developmentRemoteFunctions",
        "serve:portal2": "nx serve portal-react",
        "deploy:functions": "firebase deploy",
        "serve:playground": "cd apps/functions/src && http-server",
        "format:everything": "prettier --write .",
        "install:all": "npm ci && husky install && node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
        "analyze": "webpack-bundle-analyzer public/stats.json"
    },
