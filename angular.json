                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/firebase/database"],
                    "options": {
                        "jestConfig": "libs/firebase/database/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
